package projetoFaculdade;
//package projetoFaculdade;

public class EmpregadoTeste {

	public static void main(String[] args) {
		Professor p1 = new Professor("Lucas", 2000, 5);
		Professor p2 = new Professor("Marcos", 5000, 8);
		
		p1.getGastos();
		p2.getGastos();
		
		Relatorio r1 = new Relatorio();
		
		System.out.println("Relat√≥rio\n----------------------");
		r1.relatorio(p1); 
		r1.relatorio(p2);
		r1.gastos(p1);
		r1.gastos(p2);
		System.out.println("Total Gasto: " + r1.getGastosTotais());
		
		
	}

}
